{
  "name": "webpack-example",
  "version": "1.0.0",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "main": "index.js",
  "repository": "https://github.com/KumoIs/webpack-example.git",
  "author": "zz",
  "license": "MIT",
  "scripts": {
    "dev": "webpack --config ./config/webpack.common.js",
    "start": "webpack-dev-server --config ./config/webpack.common.js",
    "build": "webpack --env.production --config ./config/webpack.common.js",
    "prod": "http-server -o dist",
    "build:dll": "webpack --config ./config/webpack.dll.js"
  },
  "moudleNameMapper": {
    "@": "<rootDir>/src",
    "utils": "<rootDir>/src/utils",
    "pages": "<rootDir>/src/pages",
    "components": "<rootDir>/src/component"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "@loadable/component": "^5.12.0",
    "chalk": "^4.1.0",
    "core-js": "3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "autoprefixer": "^9.8.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "csv-loader": "^3.0.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.3",
    "less-loader": "^6.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^1.2.1",
    "thread-loader": "^2.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^5.1.3",
    "xml-loader": "^1.2.1"
  }
}
